---
import {getCollection} from "astro:content";

import Layout from "../layouts/Layout.astro";
import LayoutCard from "../components/LayoutCard.astro";
import PhotoGrid from "../components/PhotoGrid.astro";

const images = await getCollection('landscapes');
const duplicated = images.concat(images);
---

<Layout title="Portrait Photography by Jo Maendle">
    <LayoutCard title="Nature"
                subtitle="Explore some the beauty of our world.">

        <PhotoGrid images={duplicated}/>
    </LayoutCard>
</Layout>
